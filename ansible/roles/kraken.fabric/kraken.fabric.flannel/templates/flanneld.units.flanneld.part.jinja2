{% if item.containerConfig.fabric == 'flanneld' %}
- name: flanneld.service
  command: start
  drop-ins:
    - name: 50-network-config.conf
      content: |
        [Unit]
        Requires=etcd3.service
        [Service]
        ExecStartPre=/usr/bin/etcdctl set /coreos.com/network/config '{"Network":{{item.clusterNetwork}}, "Backend": {"Type": {{item.clusterNetworkBackend}}}
  command: start
{% endif %}
